{
  "name": "egg-example",
  "version": "1.0.0",
  "description": "node devops",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-egg-example",
    "stop": "egg-scripts stop --title=egg-server-egg-example",
    "dev": "egg-bin dev --sticky",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "eslint . --ext .ts",
    "clean": "ets clean"
  },
  "dependencies": {
    "@types/jenkins": "^0.23.3",
    "egg": "^2.6.1",
    "egg-cors": "^2.2.3",
    "egg-helper": "^1.1.5",
    "egg-jwt": "^3.1.7",
    "egg-scripts": "^2.6.0",
    "egg-sequelize": "^6.0.0",
    "egg-shell-decorators": "^1.6.0",
    "egg-socket.io": "^4.1.6",
    "jenkins": "^0.28.1",
    "marked": "^4.0.15",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.4",
    "nunjucks": "^3.2.3",
    "sequelize": "^6.19.0"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.12",
    "@types/supertest": "^2.0.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.16.0",
    "eslint": "^6.7.2",
    "eslint-config-egg": "^8.0.0",
    "sequelize-cli": "^6.4.1",
    "tslib": "^1.9.0",
    "typescript": "^3.0.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "xiuyue",
  "license": "MIT"
}
